pragma solidity ^0.6.2;

import "https://github.com/naokicannon/C-app/blob/main/ERC20.SOL";

contract Token is ERC20 {
    
    constructor () public ERC20("Capp", "CAPP"){
    _mint(msg.sender, 1000000 * (10 ** uint256(decimals())));
    }
}
